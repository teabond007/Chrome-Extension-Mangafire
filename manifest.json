{
  "name": "Manga Bookmarks Marker",
  "description": "for MangFire. This is not official MangFire extension and is not affiliated with MangFire in any way. It colors the border of saved Mangas.",
  "version": "1.5",
  "manifest_version": 3,
  "options_ui": {
  "page": "options.html",
  "open_in_tab": true
},
  "icons": {
  "128": "favicon.png"
},
  "action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["scripting", "tabs", "storage"],
    "host_permissions": [
    "https://mangafire.to/user/bookmark"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://mangafire.to/*"],
      "js": ["content.js"]
    }
  ]
}