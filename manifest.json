{
  "name": "first Extensions",
  "description": "Base Level Extension",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_icon": "HiIcon.png"
  },
  "permissions": ["scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://mangafire.to/*"],
      "js": ["content.js"]
    }
  ]
}