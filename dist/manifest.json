{
  "name": "Bookmarks Marker/Higlighter",
  "description": "for MangFire v2. It colors the border of saved Mangas. Doesn't work on MangaFire homepage yet",
  "version": "5.0.0",
  "manifest_version": 3,
  "options_ui": {
    "page": "src/options/options.html",
    "open_in_tab": true
  },
  "icons": {
    "128": "icons/favicon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "src/newtab/newtab.html"
  },
  "action": {
    "default_icon": "icons/favicon128.png",
    "default_popup": "src/popup/popup.html"
  },
  "permissions": [
    "scripting",
    "tabs",
    "storage",
    "identity"
  ],
  "oauth2": {
    "client_id": "230078342235-anlc8rvjvbo5lg18tclldqpucfc0bja3.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.appdata",
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "host_permissions": [
    "https://mangafire.to/user/bookmark",
    "https://graphql.anilist.co/*",
    "https://api.mangadex.org/*",
    "https://mangadex.org/*"
  ],
  "optional_host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/main.ts-loader-611b8244.js"
      ],
      "matches": [
        "*://mangafire.to/*",
        "*://mangadex.org/*",
        "*://asuracomic.net/*",
        "*://asurascans.com/*",
        "*://asuratoon.com/*",
        "*://www.webtoons.com/*",
        "*://manganato.com/*",
        "*://chapmanganato.com/*",
        "*://mangaplus.shueisha.co.jp/*"
      ],
      "run_at": "document_idle",
      "css": [
        "assets/main-5b848d8d.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "icons/*",
        "images/*",
        "assets/*"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "*://asuracomic.net/*",
        "*://asurascans.com/*",
        "*://asuratoon.com/*",
        "*://chapmanganato.com/*",
        "*://mangadex.org/*",
        "*://mangafire.to/*",
        "*://manganato.com/*",
        "*://mangaplus.shueisha.co.jp/*",
        "*://www.webtoons.com/*"
      ],
      "resources": [
        "assets/preload-helper-cf010ec4.js",
        "assets/runtime-dom.esm-bundler-1d81061f.js",
        "assets/_plugin-vue_export-helper-c27b6911.js",
        "assets/generic-adapter-2a261233.js",
        "assets/main.ts-a920709e.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
