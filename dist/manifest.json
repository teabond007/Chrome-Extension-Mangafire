{
  "name": "Bookmarks Marker/Higlighter",
  "description": "for MangFire v2. It colors the border of saved Mangas. Doesn't work on MangaFire homepage yet",
  "version": "3.8.0",
  "manifest_version": 3,
  "options_ui": {
    "page": "src/options/options.html",
    "open_in_tab": true
  },
  "icons": {
    "128": "icons/favicon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "src/newtab/newtab.html"
  },
  "action": {
    "default_icon": "icons/favicon128.png",
    "default_popup": "src/popup/popup.html"
  },
  "permissions": [
    "scripting",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "https://mangafire.to/user/bookmark",
    "https://graphql.anilist.co/*",
    "https://api.mangadex.org/*",
    "https://mangadex.org/*"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/main.ts-loader-58e4117c.js"
      ],
      "matches": [
        "*://mangafire.to/*",
        "*://mangadex.org/*",
        "*://asuracomic.net/*",
        "*://asurascans.com/*",
        "*://asuratoon.com/*",
        "*://www.webtoons.com/*",
        "*://manganato.com/*",
        "*://chapmanganato.com/*",
        "*://mangaplus.shueisha.co.jp/*"
      ],
      "run_at": "document_idle",
      "css": [
        "assets/main-5f3479e4.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "icons/*",
        "images/*"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "*://asuracomic.net/*",
        "*://asurascans.com/*",
        "*://asuratoon.com/*",
        "*://chapmanganato.com/*",
        "*://mangadex.org/*",
        "*://mangafire.to/*",
        "*://manganato.com/*",
        "*://mangaplus.shueisha.co.jp/*",
        "*://www.webtoons.com/*"
      ],
      "resources": [
        "assets/runtime-dom.esm-bundler-1e3063a8.js",
        "assets/_plugin-vue_export-helper-c27b6911.js",
        "assets/main.ts-e4abbaee.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
