{
  "manifest_version": 3,
  "name": "Manga Bookmark Manager",
  "description": "Manage and highlight your manga bookmarks across multiple platforms with premium UI",
  "version": "4.0.0",
  "icons": {
    "16": "icons/logo/logo16.png",
    "48": "icons/logo/logo48.png",
    "128": "icons/logo/logo128.png"
  },
  "permissions": [
    "storage",
    "bookmarks",
    "tabs",
    "alarms",
    "scripting"
  ],
  "host_permissions": [
    "https://api.mangadex.org/*",
    "https://graphql.anilist.co/*",
    "https://mangafire.to/*"
  ],
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "icons/logo/logo16.png",
      "48": "icons/logo/logo48.png",
      "128": "icons/logo/logo128.png"
    }
  },
  "options_ui": {
    "page": "options/index.html",
    "open_in_tab": true
  },
  "chrome_url_overrides": {
    "newtab": "newtab/index.html"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.js-loader-2ixLH9xl.js"
      ],
      "matches": [
        "https://mangafire.to/*"
      ],
      "run_at": "document_idle",
      "css": [
        "assets/index-tn0RQdqM.css"
      ]
    },
    {
      "js": [
        "assets/index.js-loader-PsmicxWc.js"
      ],
      "matches": [
        "https://mangadex.org/*"
