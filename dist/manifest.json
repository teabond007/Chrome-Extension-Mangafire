{
  "name": "Bookmarks Marker/Higlighter",
  "description": "for MangFire v2. It colors the border of saved Mangas. Doesn't work on MangaFire homepage yet",
  "version": "3.8.0",
  "manifest_version": 3,
  "options_ui": {
    "page": "src/options/options.html",
    "open_in_tab": true
  },
  "icons": {
    "128": "icons/favicon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "src/newtab/newtab.html"
  },
  "action": {
    "default_icon": "icons/favicon128.png",
    "default_popup": "src/popup/popup.html"
  },
  "permissions": [
    "scripting",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "https://mangafire.to/user/bookmark",
    "https://graphql.anilist.co/*",
    "https://api.mangadex.org/*",
    "https://mangadex.org/*"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content-mangafire.src.js-loader-ea15341f.js"
      ],
      "matches": [
        "*://mangafire.to/*"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "assets/content-mangadex.src.js-loader-41cfe88f.js"
      ],
      "matches": [
        "*://mangadex.org/*"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "assets/content-asurascans.src.js-loader-b1faf349.js"
      ],
      "matches": [
        "*://asuracomic.net/*",
        "*://asurascans.com/*",
        "*://asuratoon.com/*"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "assets/content-webtoons.src.js-loader-9f718504.js"
      ],
      "matches": [
        "*://www.webtoons.com/*"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "assets/content-manganato.src.js-loader-b64b143f.js"
      ],
      "matches": [
        "*://manganato.com/*",
        "*://chapmanganato.com/*"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "assets/content-mangaplus.src.js-loader-b10a4c4e.js"
      ],
      "matches": [
        "*://mangaplus.shueisha.co.jp/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "icons/*",
        "images/*"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "*://mangafire.to/*"
      ],
      "resources": [
        "assets/reader-enhancements-db3a7f4d.js",
        "assets/content-mangafire.src.js-d520ac30.js"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "*://mangadex.org/*"
      ],
      "resources": [
        "assets/reader-enhancements-db3a7f4d.js",
        "assets/content-mangadex.src.js-4d1fa0aa.js"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "*://asuracomic.net/*",
        "*://asurascans.com/*",
        "*://asuratoon.com/*"
      ],
      "resources": [
        "assets/reader-enhancements-db3a7f4d.js",
        "assets/content-asurascans.src.js-32057910.js"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "*://www.webtoons.com/*"
      ],
      "resources": [
        "assets/reader-enhancements-db3a7f4d.js",
        "assets/content-webtoons.src.js-1bcf520e.js"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "*://chapmanganato.com/*",
        "*://manganato.com/*"
      ],
      "resources": [
        "assets/reader-enhancements-db3a7f4d.js",
        "assets/content-manganato.src.js-ccfcc45f.js"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "*://mangaplus.shueisha.co.jp/*"
      ],
      "resources": [
        "assets/reader-enhancements-db3a7f4d.js",
        "assets/content-mangaplus.src.js-928a5ff5.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
